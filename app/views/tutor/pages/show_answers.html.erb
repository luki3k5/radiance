<div id="main">
  <%= render 'layouts/breadcrumb' %>

  <h1 id="page_title"><%= render 'page_title' %></h1>
	<br/>
  <div class="showView" id="contentElements">
    <%= render :partial => 'tutor/contents/content', :collection => @page.contents %>
    <%= hidden_field_tag "page", tutor_page_contents_path(@page)%>
  </div>
	<div class="grade">		
  </div>
	<%= button_to "Enter in Coursebook", :action => "update", :confirm => "Are you sure?", :disabled_with => "Sending..." %>
	<%= hidden_field_tag "grade_hidden" %>
	<%= render 'page_bottom' %> 
</div>
<div id="aside">

</div>
<script type="text/javascript">
	$(document).ready(function(){
    Stitched.loadPageAnswerView();
		Stitched.loadGradeView();
  });
</script>