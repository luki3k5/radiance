<div id="main">
  <h1>New Deadline </h1>
  <%= form_for(@deadline, :url => tutor_course_deadlines_path(@course)) do |f| %>
    <p class="checkbox">
			<%= label :due_date, "Deadline" %><br />
			<%= datetime_select "deadline", :due_date, :default => @group_deadline.due_date %> 
		</p>
		<%= hidden_field_tag :deadlinable, @page.id %>
		<%= hidden_field_tag :group_id, @group.id %>
		<p class="submit"> 
		  <%= submit_tag "Submit" %>  
		</p>
  <% end %>
  <%= link_to 'Back', tutor_course_deadlines_path(@course) %>

</div>
<div id="aside">

</div>