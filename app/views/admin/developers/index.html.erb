<div id="main">
  <h1>Listing developers</h1>

  <table  class="userlist">
    <thead>      
      <tr>
        <td>Name</td>
        <td>Lastname</td>
        <td>E-Mail</td>
        <td>&nbsp;</td>
      </tr>
    </thead>
    <tbody>
      <% @developers.each do |developer| %>
        <tr>
          <td><%= developer.profile.name %></td>
          <td><%= developer.profile.lastname %></td>
          <td><%= developer.profile.email %></td>
          <td>
            <%= link_to image_tag("space.gif", :alt => "edit Developer", :class => "icon useredit"), edit_admin_developer_path(developer) %>
            <%= link_to image_tag("space.gif", :alt => "delete Developer", :class => "icon delete"), admin_developer_path(developer), :confirm => 'Are you sure, you want to delete this Developer?', :method => :delete %>
          </td>

        </tr>
      <% end %>
      <tr>
        <td><%= link_to 'Create New Developer', new_admin_developer_path %></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
    </tbody>
  </table>
</div>
<div id="aside">

</div>
