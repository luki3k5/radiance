<div id="main">
	<h1>Listing tutors</h1>

	<table  class="userlist">
  	<thead>
  		<tr>
        <td>Name</td>
        <td>Lastname</td>
        <td>E-Mail</td>
        <td>&nbsp;</td>
      </tr>
    </thead>
    <tbody>
			<% @tutors.each do |tutor| %>
			  <tr>
          <td><%= tutor.profile.name %></td>
          <td><%= tutor.profile.lastname %></td>
          <td><%= tutor.profile.email %></td>
          <td>
            <%= link_to image_tag("space.gif", :alt => "edit Tutor", :class => "icon useredit"), edit_admin_tutor_path(tutor) %>
            <%= link_to image_tag("space.gif", :alt => "delete Tutor", :class => "icon delete"), admin_tutor_path(tutor), :confirm => 'Are you sure, you want to delete this Tutor?', :method => :delete %>
          </td>
        </tr>
			<% end %>
				<tr>
        	<td><%= link_to 'Create New Tutor', new_admin_tutor_path %></td>
	        <td></td>
	        <td></td>
	        <td></td>
	      </tr>
    </tbody>
</table>
</div>
<div id="aside"></div>
